{
  "name": "GhanaEMIS",
  "author": "Synergy International Systems",
  "authors": "Synergy International Systems",
  "url": "http://synisys.com",
  "homepage": "http://synisys.com",
  "description": "GhanaEMIS",
  "productName": "GhanaEMIS",
  "version": "1.0.11",
  "repository": "https://github.com/haykanushT/GhanaEMIS_Releases",
  "license": "ISC",
  "main": "./main.js",
  "build": {
    "appId": "GhanaEMIS",
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "app/resources/images/logo.ico",
      "publisherName": "Synergy International Systems"
    },
    "nsis": {
      "runAfterFinish": true
    }
  },
  "scripts": {
    "start": "electron .",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "rebuild": "electron-rebuild -f -w fs,path",
    "cleanElectronPackage": "rimraf ../1.0.9/packaged",
    "buildElectronPackage": "npm run cleanElectronPackage && electron-packager . --out ../1.0.9/packaged --asar --overwrite --platform=win32 --arch=x64 --icon=app/resources/images/logo.ico --version-string.CompanyName=\"Synergy International Systems\" --version-string.OriginalFilename=\"GhanaEMIS\" --version-string.ProductName=\"GhanaEMIS\"",
    "cleanElectronInstaller": "rimraf ../1.0.9/el_win_installer",
    "setupElectronInstaller": "npm run cleanElectronInstaller && electron-installer-windows --src ../1.0.9/packaged/GhanaEMIS-win32-x64/ --dest ../1.0.9/el_win_installer/ --config config.json",
    "finalElectronInstaller": "npm run buildElectronPackage && npm run setupElectronInstaller",
    "pack": "build --dir",
    "dist": "build --x64 --ia32",
    "build_for_minify": "browserify -s main app/main.js > ../gha-emis-offline-minified/appMinified/app.js && npm run minify",
    "minify": "uglifyjs ../gha-emis-offline-minified/appMinified/app.js --screw-ie8 --compress --mangle --output ../gha-emis-offline-minified/appMinified/app.min.js"
  },
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/platform-server": "2.4.1",
    "@angular/router": "3.4.1",
    "@angular/upgrade": "2.4.1",
    "@synisys/ng-controls": "1.0.1-rc-3",
    "app": "^0.1.0",
    "bluebird": "^3.4.7",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "electron-logger": "0.0.3",
    "electron-pdf-window": "~1.0.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^1.14.2",
    "express": "^4.14.0",
    "fs-promise": "^2.0.0",
    "material-design-lite": "^1.2.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.2",
    "sqlite": "^2.3.0",
    "stream": "0.0.2",
    "systemjs": "0.19.41",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.1-beta.32.3",
    "angular-mocks": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "asar": "^0.13.0",
    "awesome-typescript-loader": "^2.2.4",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "diff": "^3.0.0",
    "electron": "~1.4.1",
    "electron-builder": "^16.8.4",
    "electron-installer-windows": "^0.2.0",
    "electron-packager": "^8.3.0",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "~1.4.0",
    "electron-windows-installer": "^1.4.4",
    "electron-winstaller": "^2.5.2",
    "file-loader": "^0.8.5",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-angular2-embed-sass": "^0.4.0",
    "gulp-angular2-embed-templates": "^2.2.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-compiled-typescript": "^1.2.0",
    "gulp-concat": "^2.6.0",
    "gulp-html-replace": "^1.6.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-uglify": "^1.5.4",
    "html-loader": "^0.4.3",
    "image-webpack-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "lite-server": "^2.2.2",
    "mocha": "^3.1.0",
    "pump": "^1.0.1",
    "string-replace-loader": "^1.0.5",
    "systemjs-builder": "^0.16.4",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.2.2",
    "typings": "^1.4.0",
    "webpack": "^1.13.2"
  }
}
